{
  "buildCommand": "cd apps/web && npm run build",
  "installCommand": "npm install",
  "outputDirectory": "apps/web/.next",
  "framework": "nextjs",
  "regions": ["iad1"],
  "functions": {
    "apps/web/pages/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "env": {
    "TURBO_CACHE": "1",
    "TURBO_REMOTE_ONLY": "1"
  }
}
